[database]
classname = org.hsqldb.jdbcDriver
subprotocol = hsqldb
subname = mem:puppetdb;hsqldb.tx=mvcc;sql.syntax_pgs=true

[mq]
dir = <%= scope.lookupvar('grayskull::installdir') %>/mq
connection_string = vm://localhost
endpoint = com.puppetlabs.puppetdb.commands

[jetty]
host = <%= fqdn %>
port = <%= port %>
